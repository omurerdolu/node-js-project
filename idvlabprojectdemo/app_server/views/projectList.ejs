<html>
    <body>
        <h1>Project List Screen<br><br><br></h1>

        <table class="table table-striped table-dark">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Proje Name</th>
                <th scope="col">Phone</th>
                
              </tr>
            </thead>
            <tbody>
                    <%  for(var i=0;i<users.length;i++){%>  
        
                    
              <tr>
                <td><%=i+1  %> </td>
                <td><%=users[i].yourName %> </td>
                <td><%=users[i].projectName  %> </td>
                <td> <%=users[i].projectPhone  %></td>
               
              </tr>
              <% } %>
            </tbody>
          </table>
    </body>
</htm